{
  "name": "biatec-concentrated-liquidity-amm",
  "version": "0.9.0",
  "license": "MIT",
  "categories": [
    "Algorand",
    "DeFi",
    "Other"
  ],
  "licenses": [
    {
      "type": "AGPL-3.0-only"
    }
  ],
  "scripts": {
    "generate-client": "algokit generate client contracts/artifacts/ --language typescript  --output contracts/clients/{contract_name}Client.ts",
    "compile-contract": "tealscript contracts/*.algo.ts contracts/artifacts",
    "generate-components": "algokit-generate-component contracts/artifacts/Biatecclamm.arc32.json contracts/artifacts/components",
    "build": "npm run compile-contract && npm run generate-client",
    "test": "npm run build && jest",
    "test:1:build": "npm run build && jest -t \"Extreme-ExtremePrice-Min - ASASR 0.000000001\"",
    "test:1:nobuild": "jest -t \"Extreme-ExtremePrice-Min - ASASR 0.000000001\"",
    "test:nobuild": "jest",
    "lint": "eslint . --ext .ts",
    "fix": "eslint . --ext .ts --fix",
    "build-package": "tsup src/index.ts --format cjs,esm --clean --sourcemap index --dts src/index.ts --dts-resolve",
    "publishCmd": "npm publish"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@algorandfoundation/algokit-utils": "^5.6.0",
    "algosdk": "^2.7.0"
  },
  "devDependencies": {
    "@algorandfoundation/algokit-client-generator": "^3.0.0",
    "@algorandfoundation/tealscript": "^0.88.0",
    "@jest/globals": "^29.7.0",
    "@joe-p/algokit-generate-component": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "5.0.2"
  }
}
